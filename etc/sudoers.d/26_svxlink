#implemented in part.
svxlink ALL=(ALL) NOPASSWD: ALL
svxlink ALL=(ALL) NOPASSWD:/sbin/reboot
svxlink ALL=(ALL) NOPASSWD:/usr/sbin/halt
svxlink ALL=(ALL) NOPASSWD:/usr/bin/tail * /var/log/svxlink
svxlink ALL=(ALL) NOPASSWD:/usr/bin/wc
svxlink ALL=(ALL) NOPASSWD:/usr/bin/tr
svxlink ALL=(ALL) NOPASSWD:/usr/bin/cut
svxlink ALL=(ALL) NOPASSWD:/usr/bin/sed -i * /etc/hosts
svxlink ALL=(ALL) NOPASSWD:/usr/bin/chmod guo+rw /dev/ttyS[1-4]
svxlink ALL=(ALL) NOPASSWD:/usr/bin/chmod guo+rw /sys/class/gpio/gpio[0-99]/value
svxlink ALL=(ALL) NOPASSWD:/usr/bin/echo [0-9] > /sys/class/gpio/gpio[0-99]/value
svxlink ALL=(ALL) NOPASSWD:/bin/systemctl start svxlink.service
svxlink ALL=(ALL) NOPASSWD:/bin/systemctl stop svxlink.service
svxlink ALL=(ALL) NOPASSWD:/bin/systemctl restart svxlink.service
svxlink ALL=(ALL) NOPASSWD:/usr/bin/hostnamectl
svxlink ALL=(ALL) NOPASSWD:/usr/bin/chmod guo+rw /tmp/dtmf_svx
svxlink ALL=(ALL) NOPASSWD:/usr/bin/chmod guo+rw /tmp/SQL
svxlink ALL=(ALL) NOPASSWD:/usr/sbin/poweroff
